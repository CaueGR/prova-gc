package application;

public class Item {

	private String nome;
	private Integer quantidade;
	
	public Item(String nome) {
		this.nome = nome;
		this.quantidade = 1;
	}

	public Item(String nome, Integer quantidade) {
		this.nome = nome;
		this.quantidade = quantidade;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public Integer getQuantidade() {
		return quantidade;
	}

	
	
	
	
		

	}

}
